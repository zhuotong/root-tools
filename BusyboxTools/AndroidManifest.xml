<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.snda.root.busybox" android:versionCode="4"
	android:versionName="1.3">
	<uses-sdk android:minSdkVersion="8"></uses-sdk>
	<application android:label="@string/app_name" android:icon="@drawable/icon">
		<activity android:name="MainActivity" android:label="@string/app_name"
			android:configChanges="orientation|keyboardHidden">
			<intent-filter>
				<action android:name="SNDA.ROOT" />
				<category android:name="android.intent.category.OPENABLE" />
			</intent-filter>
		</activity>
		<activity android:name="CommandHelpActivity" android:label="@string/app_cmd_help"
			android:configChanges="orientation|keyboardHidden" />

		<activity android:name="HelpActivity" android:label="@string/app_name"
			android:configChanges="orientation|keyboardHidden" />
	</application>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
</manifest> 
